{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake: Configure",
			"type": "shell",
			"command": "cmake",
			"args": [
				"-S",
				".",
				"-B",
				"build"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "CMake: Build",
			"type": "shell",
			"command": "cmake",
			"args": [
				"--build",
				"build"
			],
			"isBackground": false,
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "CMake: Configure with Coverage",
			"type": "shell",
			"command": "cmake",
			"args": [
				"-S",
				".",
				"-B",
				"build",
				"-DENABLE_COVERAGE=ON"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "Run Test Eval",
			"type": "shell",
			"command": "/Users/blair/Source/Logo/logo3/build/test_eval",
			"args": [
				"2>&1"
			],
			"group": "test"
		},
		{
			"label": "Run All Tests",
			"type": "shell",
			"command": "ctest",
			"args": [
				"--output-on-failure"
			],
			"group": "test"
		},
		{
			"label": "CTest in Build Dir",
			"type": "shell",
			"command": "cmake",
			"args": [
				"-S",
				".",
				"-B",
				"build",
				"&&",
				"ctest",
				"--test-dir",
				"build",
				"--output-on-failure"
			],
			"group": "test"
		},
		{
			"label": "Run Outside World Test",
			"type": "shell",
			"command": "./build/test_primitives_outside_world",
			"args": [],
			"group": "test"
		},
		{
			"label": "CTest All",
			"type": "shell",
			"command": "ctest",
			"args": [
				"--test-dir",
				"build",
				"--output-on-failure"
			],
			"group": "test"
		},
		{
			"label": "Outside World Direct",
			"type": "shell",
			"command": "./build/test_primitives_outside_world",
			"args": [],
			"group": "test"
		},
		{
			"label": "CTest All Tests",
			"type": "shell",
			"command": "ctest",
			"args": [
				"--test-dir",
				"build",
				"--output-on-failure"
			],
			"group": "test"
		}
	]
}