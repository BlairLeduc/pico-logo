to ; :comment
end

to map :cmd :list
  if empty? :list [ stop ]
  run list :cmd word "" first :list
  map :cmd bf :list
end

to sort :arg :list
  if empty? :arg [ op :list ]
  make "list insert first :arg :list
  op sort bf :arg :list
end

to insert :a :l
  if empty? :l [ op ( list :a ) ]
  if before? :a first :l [op fput :a :l ]
  op fput first :l insert :a bf :l
end
