to divisor? :a :b
  op 0 = remainder :b :a
end

to intpwr :a :intp
  if or :a = 1 :intp = 0 [op 1]
  if :intp < 0 [ op 1 / pwrloop (:a) (-:intp) ]
  op pwrloop :a :intp
end

to pwrloop :a :intp
  if :intp = 0 [op 1]
  op :a * pwrloop :a :intp - 1
end

to efrac :frac :count :term
  if :count > 9 [op 0]
  make "term :term * :frac / :count
  op :term + efrac :frac :count + 1 :term
end